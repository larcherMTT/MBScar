<Group labelreference="L256816" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Describe(MBScar):</Text-field>
</Input>
<Output><Text-field style="Line Printed Output" layout="Line Printed Output">
# A package for the generation of the equations of motion of a multibody system 
# in minimal coordinate and quasi-velocities form. The theory behind the 
# package is based on the definitions found in the book 'Advanced Dynamics' by 
# Donald T. Greenwood.
# This moule extend the functionalities of the MBSymba_r6 
# module by R.Lot (c)2003-2011 and M.Massaro (c).
module MBScar:

    # Print module information.
    Info( )

    # Module load procedure.
    ModuleLoad( )

    # Module unload procedure.
    ModuleUnload( )

    # Set the module options: warning mode &lt;WarningMode&gt;, time limit 
    # &lt;TimeLimit&gt;.
    SetModuleOptions( { ParallelMode::{boolean, nothing} := NULL, 
                        TimeLimit::{nothing, nonnegative} := NULL, 
                        WarningMode::{boolean, nothing} := NULL }, $ )

    # Check if the variable &lt;var&gt; is of FRAME type.
    IsFRAME( var::anything, $ ) :: boolean

    # Check if the variable &lt;var&gt; is of BODY type.
    IsBODY( var::anything, $ ) :: boolean

    # Check if the variable &lt;var&gt; is of FORCE type.
    IsFORCE( var::anything, $ ) :: boolean

    # Check if the variable &lt;var&gt; is of TORQUE type.
    IsTORQUE( var::anything, $ ) :: boolean

    # Check if the variable &lt;var&gt; is of POINT type.
    IsPOINT( var::anything, $ ) :: boolean

    # Check if the variable &lt;var&gt; is of VECTOR type.
    IsVECTOR( var::anything, $ ) :: boolean

    # Check if the two objects &lt;obj1&gt; and &lt;obj2&gt; are equal.
    IsEqual( obj1::anything, obj2::anything, $ ) :: boolean

    # Try to simplify an algebraic expression &lt;var&gt; with optional 
    # simplification options &lt;opt&gt;. The simplification is performed within the 
    # internal or indexed time limit.
    Simplify( var::anything, opt::anything := NULL, $ ) :: anything

    # Plot of non-zero values of matrix &lt;A&gt;. The optional arguments 
    # &lt;row_labels&gt; and &lt;col_labels&gt; are used to label the rows and columns of 
    # the matrix.
    Spy( A::Matrix, 
         { col_labels::{list, nothing} := [seq(i,i = 1 .. 
             LinearAlgebra:-ColumnDimension(A))], 
           row_labels::{list, nothing} := [seq(i,i = 1 .. 
             LinearAlgebra:-RowDimension(A))] }, 
         $ ) :: anything

    # Inverse affine transformation matrix &lt;RF&gt;.
    InverseFrame( RF::MBSymba_FRAME, $ ) :: MBSymba_FRAME

    # Project the vector/point/force/torque &lt;x&gt; from the reference frame 
    # &lt;RF_ini&gt; to the reference frame &lt;RF_end&gt;.
    Project( obj::{MBSymba_FORCE, MBSymba_POINT, MBSymba_TORQUE, 
               MBSymba_VECTOR}, 
             RF_end::MBSymba_FRAME, $ )
           :: {MBSymba_FORCE, MBSymba_POINT, MBSymba_TORQUE, MBSymba_VECTOR}

    # Solve the quasi-velocities equations &lt;qv_eqns&gt; for the quasi-velocities 
    # variables &lt;q_vars&gt;.
    qv_solve( qv_eqns::list({`=`, algebraic}), q_vars::list(scalar), 
              { constr::list(scalar) := [NULL] }, $ ) :: list(algebraic)

    # Substitute the generalized coordinates with the quasi-velocities in the 
    # expression &lt;expr&gt;.
    qu_subs( expr::anything, q_vars::list(scalar), 
             qv_eqns::list({`=`, algebraic}), 
             { constr::list(scalar) := [NULL] }, $ ) :: anything

    # Compute the kinetic energy of the system in terms of the 
    # quasi-velocities.
    kinetic_energy_qv( bodies::{list(MBSymba_BODY), set(MBSymba_BODY)}, 
                       q_vars::list(scalar), qv_eqns::list({`=`, algebraic}), 
                       { constr::list(scalar) := [NULL] }, $ ) :: algebraic

    # Compute the linear velocity of the body terms of the quasi-velocities.
    linear_velocity_qv( body::MBSymba_BODY, q_vars::list(scalar), 
                        qv_eqns::list({`=`, algebraic}), 
                        { constr::list(scalar) := [NULL] }, $ )

    # Compute the angular velocity of the body terms of the quasi-velocities.
    angular_velocity_qv( body::MBSymba_BODY, q_vars::list(scalar), 
                         qv_eqns::list({`=`, algebraic}), 
                         { constr::list(scalar) := [NULL] }, $ )

    # Compute the linear momentum of the body terms of the quasi-velocities.
    linear_momentum_qv( body::MBSymba_BODY, q_vars::list(scalar), 
                        qv_eqns::list({`=`, algebraic}), 
                        { constr::list(scalar) := [NULL] }, $ ) :: algebraic

    # Compute the angular momentum of the body terms of the quasi-velocities.
    angular_momentum_qv( body::MBSymba_BODY, q_vars::list(scalar), 
                         qv_eqns::list({`=`, algebraic}), 
                         { constr::list(scalar) := [NULL] }, $ ) :: algebraic

    # Compute the fundamental equations of motion of the system.
    fundamental_equations( mbo::{list({MBSymba_BODY, MBSymba_FORCE, 
                             MBSymba_TORQUE}), set({MBSymba_BODY, 
                             MBSymba_FORCE, MBSymba_TORQUE})}, 
                           q_vars::list(scalar), u_vars::list(scalar), 
                           qv_eqns::list({`=`, algebraic}), 
                           { constr::list(scalar) := [NULL] }, $ )
                         :: list(algebraic)

</Text-field>
</Output>
</Group>
